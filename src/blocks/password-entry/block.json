{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "password-protect-elite/password-entry",
	"version": "1.0.0",
	"title": "Password Entry",
	"category": "common",
	"icon": "lock",
	"description": "A form for users to enter a password and get redirected.",
	"keywords": ["password", "form", "protection", "security", "login", "authentication"],
	"textdomain": "password-protect-elite",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./editor.css",
	"style": "file:./style.css",
	"render": "file:./render.php",
	"attributes": {
		"allowedGroups": {
			"type": "array",
			"default": []
		},
		"buttonText": {
			"type": "string",
			"default": "Submit"
		},
		"placeholder": {
			"type": "string",
			"default": "Enter password"
		},
		"redirectUrl": {
			"type": "string",
			"default": ""
		},
		"className": {
			"type": "string",
			"default": ""
		}
	},
	"supports": {
		"html": false,
		"className": true,
		"customClassName": true
	}
}
